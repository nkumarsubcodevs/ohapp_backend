<% include ../partials/header %>
  <% include ../partials/leftpart %>
    <% console.log('test') %>
      <div class="content-wrapper">
        <section class="content-header">
          <h1>
            Feedback details
          </h1>
          <ol class="breadcrumb">
            <li><a href="/"><i class="fa fa-dashboard"></i>Home</a></li>
            <li class="active">Feedback details</li>
          </ol>
        </section>

        <section class="content">
          <% include ../partials/messagebox %>
            <div class="row">
              <div class="col-xs-12">
                <div class="box">
                  <div class="box-body">
                    <table class="table table-bordered table-striped table-hover">
                      <tbody>
                        <tr>
                          <th style="width: 10px">ID</th>
                          <th>FIRST NAME</th>
                          <th>LAST NAME</th>
                          <th>EMAIL</th>
                          <th>MESSAGE</th>
                          <th>CREATE TIME</th>
                          <th>UPDATE TIME</th>
                          <th>ACTION</th>
                        </tr>

                        <% if(typeof pageDataValue !='undefined' ) { pageDataValue.forEach(function(item) {
                          if(item.user) { %>
                          <tr>
                            <td>
                              <%=item.id%>
                            </td>
                            <td>
                              <%=item.user.first_name%>
                            </td>
                            <td>
                              <%=item.user.last_name%>
                            </td>
                            <td>
                              <%=item.user.email%>
                            </td>
                            <td>
                              <%=item.feedback_details%>
                            </td>
                            <td>
                              <%= custom_helper.h_convertDateTimeInTimeStamp(item.create_time)%>
                            </td>
                            <td>
                              <%= custom_helper.h_convertDateTimeInTimeStamp(item.update_time)%>
                            </td>
                          </tr>
                          <% } }); } %>
                      </tbody>
                    </table>
                  </div>
                  <div class="box-footer clearfix">
                    <% include ../partials/paginate %>
                  </div>
                </div>


              </div>
            </div>
        </section>
      </div>
      <% include ../partials/footer %>