<body class="hold-transition login-page">   

<div class="container-fluid">
		<div class="row">
		  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 authfy-panel-left" style="background: linear-gradient(to bottom, rgba(42,188,246,1) 0%, rgba(246,81,205,1) 100%); display: flex; justify-content: center;">
			<div style="width: 35%;">
				<!-- brand-logo start -->
				<div class="brand-logo text-center">
				 <a href="/"><img src="/images/Group.png" width="150px"></a>
				</div><!-- ./brand-logo -->

				<% include ../partials/messagebox %>

				<!-- authfy-login start -->
				<div class="authfy-register">
				  <!-- panel-login start -->
				  <div class="authfy-panel panel-login text-center active">
					<div class="authfy-heading">
					  <h3 class="auth-title" style="color: blanchedalmond;">Create Your Account.</h3>
					</div>
					<div class="row">
					  <div class="col-xs-12 col-sm-12">
		  			<form method="post" action="/users/register">
						<div class="form-group">
			  				<label class="col-md-4" style="padding-left:0;text-align: left;">Name</label>
			  				<input type="text" class="form-control" name="name"/>
						</div>
						<div class="form-group">
							<label class="col-md-4" style="padding-left:0;text-align: left;">Email</label>
							<input type="text" class="form-control" name="email"/>
						</div>
						<div class="form-group">
							<label class="col-md-4" style="padding-left:0;text-align: left;">Password</label>
							<input type="password" class="form-control" name="password"/>
						</div>
						<div class="form-group">
							<label class="col-md-4" style="padding-left:0;text-align: left;">Confirm Password</label>
							<input type="password" class="form-control" name="cfm_pwd"/>
						</div>
						<div class="form-group">
							<button type="submit" class="btn btn-primary">Add User</button>
						</div>
		  			</form>
				</div>
			  </div>
			</div> <!-- ./panel-login -->
			<div class="authfy-panel panel-forgot">

			</div> <!-- ./panel-forgot -->
		  </div> <!-- ./authfy-login -->
	  </div>
	</div> <!-- ./authfy-panel-left -->
  </div> <!-- ./row -->
</div> <!-- ./container -->
<!-- /.login-box -->

<!-- /.login-box -->

<!-- jQuery 3 -->
<script src="/admtheme/bower_components/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap 3.3.7 -->
<script src="/admtheme/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<!-- iCheck -->
<script src="/admtheme/plugins/iCheck/icheck.min.js"></script>
<script src="/admtheme/dist/js/jquery.validate.min.js"></script>
<script src="/admtheme/dist/js/form_validation.js"></script>
</body>
</html>
